<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
    </style>

    <style name="BillEditText">
        <item name="android:textSize">18.0sp</item>
        <item name="android:textColor">#ff000000</item>
        <item name="android:textColorHint">@color/black</item>
        <item name="android:ellipsize">end</item>
        <item name="android:background">@null</item>
        <item name="android:paddingTop">5.0dip</item>
        <item name="android:paddingBottom">5.0dip</item>
        <item name="android:layout_width">160.0dip</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="Charge_choose_time_dialog">
        <item name="android:textSize">18.0sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/common_list_click_selector</item>
        <item name="android:paddingTop">10.0dip</item>
        <item name="android:paddingBottom">10.0dip</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="Charge_detail_subTitle">
        <item name="android:textSize">18.0sp</item>
        <item name="android:textColor">@color/common_subtitle_click_selector</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingTop">8.0dip</item>
        <item name="android:paddingBottom">8.0dip</item>
        <item name="android:layout_width">0.0dip</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1.0</item>
    </style>
    <style name="CustomCheckboxTheme" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:button">@drawable/cricle_checkbox_selector</item>
    </style>
    <style name="DialogNoAnimation">
        <item name="android:windowEnterAnimation">@null</item>
        <item name="android:windowExitAnimation">@null</item>
    </style>
    <style name="Export_bill_item">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:gravity">left</item>
        <item name="android:background">@drawable/custom_export_text_border</item>
        <item name="android:paddingLeft">3.0dip</item>
        <item name="android:paddingTop">12.0dip</item>
        <item name="android:paddingBottom">12.0dip</item>
        <item name="android:layout_width">100.0dip</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="User_subTitle">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">@color/common_subtitle_click_selector</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingTop">8.0dip</item>
        <item name="android:paddingBottom">8.0dip</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="choose_country_item_root">
        <item name="android:background">@drawable/common_list_click_selector</item>
        <item name="android:paddingLeft">15.0dip</item>
        <item name="android:paddingTop">12.0dip</item>
        <item name="android:paddingRight">25.0dip</item>
        <item name="android:paddingBottom">12.0dip</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="common_line">
        <item name="android:background">@color/common_line_color</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">0.6600001dip</item>
    </style>
    <style name="list_view" parent="@android:style/Widget.ListView">
        <item name="android:scrollbarThumbHorizontal">@drawable/nim_scrollbar_handle_holo_dark</item>
        <item name="android:scrollbarThumbVertical">@drawable/nim_scrollbar_handle_holo_dark</item>
    </style>
    <style name="register_left_font">
        <item name="android:textSize">18.0sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:layout_width">85.0dip</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="tab_item_background">
        <item name="android:textAppearance">@style/tab_item_text_style</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:paddingTop">4.0dip</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">50.0dip</item>
        <item name="android:button">@null</item>
        <item name="android:drawablePadding">0.0dip</item>
        <item name="android:layout_weight">1.0</item>
    </style>
    <style name="tab_item_text_style">
        <item name="android:textSize">13.0sp</item>
        <item name="android:ellipsize">marquee</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="translateDialogStyle" parent="@android:style/Animation">
        <item name="android:windowEnterAnimation">@anim/dialog_up_enter</item>
        <item name="android:windowExitAnimation">@anim/dialog_down_exit</item>
    </style>

    <style name="MyDialog" parent="@android:style/Theme.Dialog">
        <item name="android:backgroundDimAmount">0.6</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>

</resources>
